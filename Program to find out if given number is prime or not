
#include<stdio.h>
int main(){
    int n;
    printf("enter the number: ");
    scanf("%d",&n);
    int a = 0;
    for(int i = 2 ;i<=n-1;i++){
        if(n%i==0){  //// i is factor of n
            a = 1;
            break;
        }
        if(n==1) printf("1 is neither primr or composite number");
        else if(a==0)printf(" the given nuber is prime number");
        else("the given number s composite number");

    }
    return 0;
}
